  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/three": "^0.139.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "lamina": "^1.1.16",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "rxjs": "^7.5.5",
    "svelte": "^3.44.0",
    "svelte-aria-dialog": "^1.0.0",
    "svelte-check": "^2.2.6",
    "svelte-cubed": "^0.2.1",
    "svelte-fuse-rx": "^0.0.104",
    "svelte-preprocess": "^4.10.1",
    "three": "^0.139.2",
